@model IEnumerable<WebApplication_04.Model.Model.PostAdmission>

<link href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.0/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
<script src="//maxcdn.bootstrapcdn.com/bootstrap/3.3.0/js/bootstrap.min.js"></script>
<script src="//code.jquery.com/jquery-1.11.1.min.js"></script>
<!------ Include the above in your HEAD tag ---------->

<link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.4.0/css/font-awesome.min.css" rel='stylesheet' type='text/css'>
<link href="~/Contents/CSS/Admin/ViewPost.css" rel="stylesheet" />

@{
    Html.RenderPartial("~/Views/Shared/Student/_StudentHome.cshtml");
}
<div style="margin-left:150px; margin-top:-50px;">
    <div class="container">
        <div class="row">
            <div class="col-md-10 col-md-offset-1">

                <div class="panel panel-default panel-table">
                    <div class="panel-heading">
                        <div class="row">
                            <div class="col col-xs-6">
                                <h3 class="panel-title">Post Of Admission Notice</h3>
                            </div>
                        </div>
                    </div>
                    <div class="panel-body">
                        <table class="table table-striped table-bordered table-list">
                            <thead>
                                <tr>
                                    <th><em class="fa fa-cog"></em></th>
                                    <th>SI</th>
                                    <th>@Html.DisplayNameFor(model => model.UniversityName)</th>
                                    <th>@Html.DisplayNameFor(model => model.PostType)</th>
                                    <th>@Html.DisplayNameFor(model => model.PostDate)</th>
                                    <th>@Html.DisplayNameFor(model => model.StartTest)</th>
                                    <th>@Html.DisplayNameFor(model => model.EndTest)</th>
                                    <th>@Html.DisplayNameFor(model => model.TotalSeat)</th>
                                    <th>@Html.DisplayNameFor(model => model.FilePath)</th>
                                </tr>
                            </thead>
                            <tbody>
                                @{ int i = 1;}
                                @foreach (var item in Model)
                                {
                                    <tr>
                                        <td>
                                            @Html.ActionLink(".", "DownloadFile", new { id = item.Id }, new { @class = "fa fa-Download , btn btn-default" })
                                        </td>
                                        <td>@(i++)</td>
                                        <td>@Html.DisplayFor(modelItem => item.UniversityName)</td>
                                        <td>@Html.DisplayFor(modelItem => item.PostType)</td>
                                        <td>@Html.DisplayFor(modelItem => item.PostDate)</td>
                                        <td>@Html.DisplayFor(modelItem => item.StartTest)</td>
                                        <td>@Html.DisplayFor(modelItem => item.EndTest)</td>
                                        <td>@Html.DisplayFor(modelItem => item.TotalSeat)</td>
                                        <td>@Html.DisplayFor(modelItem => item.FilePath)</td>
                                    </tr>
                                }
                            </tbody>
                        </table>

                    </div>
                    <div class="panel-footer">
                        <div class="row">
                            <div class="col col-xs-4">
                                Page 1 of 5
                            </div>
                            <div class="col col-xs-8">
                                <ul class="pagination hidden-xs pull-right">
                                    <li><a href="#">1</a></li>
                                    <li><a href="#">2</a></li>
                                    <li><a href="#">3</a></li>
                                    <li><a href="#">4</a></li>
                                    <li><a href="#">5</a></li>
                                </ul>
                                <ul class="pagination visible-xs pull-right">
                                    <li><a href="#">«</a></li>
                                    <li><a href="#">»</a></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </div>
</div>