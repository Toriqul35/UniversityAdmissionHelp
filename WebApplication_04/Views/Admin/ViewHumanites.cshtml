@model IEnumerable<WebApplication_04.Model.Model.MajorOfHumanities>


<link href="~/Contents/CSS/Admin/MajorBusiness.css" rel="stylesheet" />
<link href="//netdna.bootstrapcdn.com/bootstrap/3.0.0/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
<script src="//netdna.bootstrapcdn.com/bootstrap/3.0.0/js/bootstrap.min.js"></script>
<script src="//code.jquery.com/jquery-1.11.1.min.js"></script>
<meta name="viewport" content="width=device-width" />
<title>Add</title>


<from class="so">
    <table>

        <tr>
            <th>Sl</th>
            <th>Total SSC</th>
            <th>Bangla1st</th>
            <th>Bangla2nd</th>
            <th>English1st</th>
            <th>English2nd</th>
            <th>Ict</th>
            <th>Economics1st</th>
            <th>Economics2nd</th>
            <th>Geography1st</th>
            <th>Geography2nd</th>
            <th>History1st</th>
            <th>History2nd</th>
            <th>Sociology1st</th>
            <th>Sociology1st</th>
            <th> <a href="~/Admin/Index"> Home</a></th>
        </tr>
        @{ int i = 1;}
        @foreach (var purchase in Model)
        {
            <tr>
                <td>@(i++)</td>
                <td>@purchase.TotalSSC</td>
                <td>@purchase.Bangla1st</td>
                <td>@purchase.Bangla2nd</td>
                <td>@purchase.Engish1st</td>
                <td>@purchase.English2nd</td>
                <td>@purchase.Ict</td>
                <td>@purchase.Economics1st</td>
                <td>@purchase.Economics2nd</td>
                <td>@purchase.Geography1st</td>
                <td>@purchase.Geography2nd</td>
                <td>@purchase.IslamicHistory1st</td>
                <td>@purchase.IslamicHistory2nd</td>
                <td>@purchase.Sociology1st</td>
                <td>@purchase.Sociology2nd</td>
                <td>
                    @Html.ActionLink("Delete", "DeleteHumanities", "Admin", new { Id = purchase.Id }, null)
                </td>
            </tr>
        }

    </table>

</from>


<script src="~/Scripts/jquery-1.8.0.min.js"></script>
<script src="~/Scripts/jquery.validate.min.js"></script>
<script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>

