        <td>@purchase.Accounting1st</td>
        <td>@purchase.Accounting2nd</td>
        <td>@purchase.Managment1st</td>
        <td>@purchase.Managment2nd</td>
        <td>@purchase.Finaance1st</td>
        <td>@purchase.Finaance2nd</td>
        <td>@purchase.Marketing1st</td>
        <td>@purchase.Marketing2nd</td>



    </tr>
        }

    </table>

</from>


