<meta name="viewport" content="width=device-width" />
<title>Add</title>


<from class="so">
    <table>

        <tr>
            <th>Sl</th>
            <th>Total SSC</th>
            <th>Bangla1st</th>
            <th>Bangla2nd</th>
            <th>English1st</th>
            <th>English2nd</th>
            <th>Ict</th>
            <th>Accounting1st</th>
            <th>Accounting2nd</th>
            <th>Managment1st</th>
            <th>Managment2nd</th>
            <th>Finaance1st</th>
            <th>Finaance2nd</th>
            <th>Marketing1st</th>
            <th>Marketing2nd</th>

        </tr>
        @{ int i = 1;}
        @foreach (var purchase in Model)
        {
    <tr>
        <td>@(i++)</td>
        <td>@purchase.TotalSSC</td>
        <td>@purchase.Bangla1st</td>
        <td>@purchase.Bangla2nd</td>
        <td>@purchase.Engish1st</td>
        <td>@purchase.English2nd</td>
